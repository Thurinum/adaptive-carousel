{
	"compilerOptions": {
		"noImplicitAny": true,
		"noEmitOnError": true,
		"removeComments": true,
		"sourceMap": true,
		"declaration": true,
		"strict": true,
		"target": "ES2022",
		"outDir": "build",
		"forceConsistentCasingInFileNames": true,
		"types": [
			"node",
			"webpack-env"
		]
	},
	"include": ["src"],
	"exclude": ["node_modules"],
	"compileOnSave": true
}
