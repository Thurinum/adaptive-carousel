{
	"compilerOptions": {
		"noImplicitAny": true,
		"noEmitOnError": true,
		"removeComments": true,
		"sourceMap": true,
		"declaration": true,
		"strict": true,
		"target": "es5",
		"outDir": "lib",
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src"],
	"exclude": ["node_modules"],
	"compileOnSave": true
}
